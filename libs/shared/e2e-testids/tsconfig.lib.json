{
  "extends": "../../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": "src",
    "outDir": "dist",
    "tsBuildInfoFile": "dist/tsconfig.lib.tsbuildinfo",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "module": "es2022",
    "moduleResolution": "bundler",
    "forceConsistentCasingInFileNames": true,
    "types": [
      "node"
    ],
    "emitDeclarationOnly": false
  },
  "include": [
    "src/**/*.ts"
  ],
  "references": []
}
