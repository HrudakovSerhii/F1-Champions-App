{
  "name": "@f1-app/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build:web-app": "npx nx build web-app && node scripts/copy-fonts.js",
    "serve:web-app": "npx nx dev web-app",
    "dev:web-app": "npx nx dev web-app",
    "build:mobile-app": "npx nx build mobile-app",
    "serve:mobile-app": "npx nx dev mobile-app",
    "dev:mobile-app": "npx nx dev mobile-app",
    "copy-fonts": "node scripts/copy-fonts.js",
    "serve:backend": "npx nx serve @f1-app/backend",
    "build:backend": "npx nx build @f1-app/backend",
    "dev:backend": "npx nx serve @f1-app/backend",
    "test:backend": "npx nx test @f1-app/backend",
    "generate-types": "./scripts/generate-types.sh",
    "web-app:init": "npm install && npm run generate-types",
    "db:generate": "cd apps/backend && npx prisma generate",
    "db:push": "cd apps/backend && npx prisma db push",
    "db:pull": "cd apps/backend && npx prisma db pull",
    "db:studio": "cd apps/backend && npx prisma studio",
    "db:seed:generate": "cd apps/backend && npx ts-node --project scripts/tsconfig.json scripts/generate-seed.ts",
    "db:seed:generate:json": "cd apps/backend && npx ts-node --project scripts/tsconfig.json scripts/generate-seed.ts json",
    "db:seed:generate:prisma": "cd apps/backend && npx ts-node --project scripts/tsconfig.json scripts/generate-seed.ts prisma",
    "db:seed:generate:mongodb": "cd apps/backend && npx ts-node --project scripts/tsconfig.json scripts/generate-seed.ts mongodb",
    "db:seed:run": "cd apps/backend && npx prisma db seed",
    "db:reset": "cd apps/backend && npx prisma db push --force-reset",
    "db:init": "npm run db:generate && npm run db:push",
    "db:setup": "npm run db:init && npm run db:seed:generate:prisma && npm run db:seed:run",
    "db:dev:setup": "npm run db:setup",
    "db:test:setup": "npm run db:reset && npm run db:seed:generate:prisma && npm run db:seed:run"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^10.0.2",
    "@nestjs/core": "^10.0.2",
    "@nestjs/platform-express": "^10.0.2",
